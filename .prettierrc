{
    "useTabs": false,
    "singleQuote": true,
    "trailingComma": "none",
    "printWidth": 100,
    "plugins": ["prettier-plugin-svelte"],
    "overrides": [
        {
            "files": "*.svelte",
            "options": {
                "parser": "svelte"
            }
        }
    ],
    "svelteSortOrder": "options-scripts-styles-markup",
    "svelteStrictMode": true,
    "svelteAllowShorthand": false,
    "svelteIndentScriptAndStyle": true
}
